如何封装一个js文件为npm包

为这个函数写一个函数注释function checkvideotime(){
    var video_buffered = video_sourceBuffer.buffered;
    var audio_buffered = audio_sourceBuffer.buffered;
    var video = document.getElementById("video_box");
    if (video_buffered.length > 0 && audio_buffered.length > 0) {
        var v_diff, a_diff;
        var v_id = video_buffered.length - 1;
        var a_id = audio_buffered.length - 1;
        v_diff = video_buffered.end(v_id) - video.currentTime;
        a_diff = audio_buffered.end(a_id) - video.currentTime;

        if (a_diff - v_diff > 0.10) {
            //console.log("we will lose an audio segment.")
            bLost = true;
        }
    }
}

